{
  "watch_directory": ".",
  "problems": {
    "solution.py": {
      "name": "第K大元素問題",
      "description": "給定一個整數陣列和數字K，找出第K大的元素",
      "test_cases": [
        {
          "name": "基本測試案例",
          "input": "5 3\n1 2 3 4 5",
          "expected_output": "3"
        },
        {
          "name": "相同數字測試",
          "input": "3 2\n10 20 30",
          "expected_output": "20"
        },
        {
          "name": "負數測試",
          "input": "4 1\n-1 -5 -3 -2",
          "expected_output": "-1"
        }
      ]
    },
    "two_sum.py": {
      "name": "兩數之和問題",
      "description": "給定一個整數陣列和目標值，找出相加等於目標值的兩個數字的索引",
      "test_cases": [
        {
          "name": "基本測試案例",
          "input": "4\n2 7 11 15\n9",
          "expected_output": "0 1"
        },
        {
          "name": "目標值為0",
          "input": "3\n-1 0 1\n0",
          "expected_output": "0 2"
        },
        {
          "name": "相同數字",
          "input": "4\n3 3 4 5\n6",
          "expected_output": "0 1"
        }
      ]
    },
    "fibonacci.py": {
      "name": "費波納奇數列",
      "description": "計算第n個費波納奇數",
      "test_cases": [
        {
          "name": "小數值測試",
          "input": "5",
          "expected_output": "5"
        },
        {
          "name": "邊界測試",
          "input": "0",
          "expected_output": "0"
        },
        {
          "name": "較大數值測試",
          "input": "10",
          "expected_output": "55"
        }
      ]
    },
    "prime_check.py": {
      "name": "質數判斷",
      "description": "判斷給定的數字是否為質數",
      "test_cases": [
        {
          "name": "質數測試",
          "input": "17",
          "expected_output": "True"
        },
        {
          "name": "非質數測試",
          "input": "15",
          "expected_output": "False"
        },
        {
          "name": "邊界測試 - 2",
          "input": "2",
          "expected_output": "True"
        },
        {
          "name": "邊界測試 - 1",
          "input": "1",
          "expected_output": "False"
        }
      ]
    },
    "sorting_algorithm.py": {
      "name": "排序演算法",
      "description": "實現並測試各種排序演算法",
      "test_cases": [
        {
          "name": "基本排序測試",
          "input": "5\n64 34 25 12 22",
          "expected_output": "12 22 25 34 64"
        },
        {
          "name": "單一元素",
          "input": "1\n42",
          "expected_output": "42"
        },
        {
          "name": "已排序陣列",
          "input": "4\n1 2 3 4",
          "expected_output": "1 2 3 4"
        },
        {
          "name": "逆序陣列",
          "input": "4\n4 3 2 1",
          "expected_output": "1 2 3 4"
        }
      ]
    },
    "matrix_operations.py": {
      "name": "矩陣運算",
      "description": "矩陣的基本運算操作",
      "test_cases": [
        {
          "name": "2x2矩陣加法",
          "input": "2 2\n1 2\n3 4\n2 2\n5 6\n7 8",
          "expected_output": "6 8\n10 12"
        },
        {
          "name": "3x3單位矩陣",
          "input": "3 3\n1 0 0\n0 1 0\n0 0 1",
          "expected_output": "1 0 0\n0 1 0\n0 0 1"
        }
      ]
    }
  }
}